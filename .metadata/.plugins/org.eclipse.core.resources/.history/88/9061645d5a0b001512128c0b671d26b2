package s1;
import org.junit.Assert;
import org.junit.Before;
import org.junit.Test;
public class SleepinTestcase {

	Sleep sl;
	
	@Before
	public void setup(){
		sl = new Sleep ();
	
	}
	
	@Test
	public void  sleepIn(){
		boolean weekday;
		boolean vacation;
		boolean expected=true;
		boolean actual = sl.sleepIn(weekday,vacation);
		if(!weekday || vacation)
		{
			
			System.out.println("If it is macthing "+actual);
		}
		Assert.assertEquals(actual, expected);
	}

		
	
	
	
}
